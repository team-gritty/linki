<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.linki.admin_integration_service.domain.dashboard.repository.myBatis.ContractStatusMapper">

    <select id="getContractStatusMapper" resultType="com.linki.admin_integration_service.domain.dashboard.dto.ContractStatusMapperDTO">
    SELECT
        contract_status AS contractStatus,
        contract_start_date AS startDate,
        contract_end_date AS endDate,
        contract_created_at AS contractCreatedDate
    FROM contract
    </select>


</mapper>